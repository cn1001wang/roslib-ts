{
  "name": "roslib-ts",
  "version": "1.0.0-beta.1", 
  "description": "Stable ROSLIB with EnhancedRos (Beta)",
  "author": "naviai",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/cn1001wang/roslib-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cn1001wang/roslib-ts.git"
  },
  "bugs": {
    "url": "https://github.com/cn1001wang/roslib-ts/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.esm.js",
      "require": "./dist/next.cjs.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "README.zh-CN.md",
    "LICENSE",
    "API.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "ros",
    "roslib",
    "robotics",
    "typescript"
  ],
  "peerDependencies": {
    "ws": "^8.0.0"
  },
  "packageManager": "pnpm@10.12.4",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "rollup": "^4.54.0",
    "rollup-plugin-dts": "^6.3.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
